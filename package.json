  "dependencies": {
    "@mkitio/gatsby-theme-password-protect": "^2.1.2",
    "gatsby": "2.24.47",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-manifest": "2.4.24",
    "gatsby-plugin-netlify-admin": "^0.1.1",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-offline": "3.2.25",
    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.3.28",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-source-google-docs": "^2.0.0-beta.18",
    "gatsby-transformer-remark": "^2.8.32",
    "gatsby-transformer-sharp": "^2.5.14",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
  },
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "yarn analyze:lint && yarn analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "yarn fix:lint && yarn fix:prettier",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "clean:drive": "rimraf src/content",
    "start": "run-p start:**",
    "start:app": "yarn develop",
    "start:lambda": "export NODE_ENV=development && netlify-lambda serve src/functions",
    "build": "yarn build:app && yarn build:lambda",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/functions"
  },
  "devDependencies": {
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.7.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-react": "7.20.6",
    "gatsby-plugin-eslint": "2.0.8",
    "http-proxy-middleware": "^1.0.5",
    "netlify": "^4.3.13",
    "netlify-lambda": "^2.0.1",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.0",
    "tailwindcss": "1.7.3"
  }
